{
  "name": "@swordev/protocol-handler",
  "version": "0.0.0",
  "description": "CLI tool for working with protocol handlers",
  "keywords": [
    "cli",
    "windows",
    "rdp",
    "protocol handler"
  ],
  "homepage": "https://github.com/swordev/protocol-handler.git",
  "bugs": {
    "url": "https://github.com/swordev/protocol-handler.git/issues",
    "email": "juanrgm724@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swordev/protocol-handler.git"
  },
  "license": "MIT",
  "author": {
    "name": "Juanra GM",
    "email": "juanrgm724@gmail.com",
    "url": "https://github.com/juanrgm"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": "lib/bin.js",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "scripts/*"
  ],
  "scripts": {
    "build": "tsc -p src",
    "format": "prettier -w ./src/**/*.ts",
    "format:check": "prettier -c ./src/**/*.ts",
    "prepare": "npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "watch": "tsc -p src -w"
  },
  "dependencies": {
    "commander": "^8.3.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.0.0",
    "@types/node": "^16.11.6",
    "conventional-changelog-cli": "^2.1.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "win32"
  ]
}
